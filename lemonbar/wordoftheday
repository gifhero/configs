    "use strict";

    var adStart = Date.now(),
        OX_dfp_ads = [["/23219321/iac.dict.dictry.dw/wotd/homepage/channel",["728x90","970x90"],"leaderboard-1",{'pos':'top','pos2':'728x90_1','adCount':'leaderboard_1',}],["/23219321/iac.dict.dictry.dw/wotd/homepage/channel",["300x250"],"medrec-1",{'pos':'top','pos2':'300x250_1','adCount':'medrec_1',}],],
        attachScriptAsync, targetingComplete ,oxDone,
        gptCallIntiated = false,
        completedTargeting = {},
        gptApp = gptApp || {},
        googletag = googletag || {},
        targetingEvtResponseTime = [],
        gptLoaded = false,
        isBlocking = false;

    gptApp.ads = gptApp.ads || [];
    googletag.cmd = googletag.cmd || [];

    attachScriptAsync = function(scriptInfo, callback) {

        /* Method to inject script dynamically to have async support */

        var tag = document.createElement("script"),
            p, node = document.getElementsByTagName("script")[0];

        tag.async = true;
        tag.onload = tag.onerror = function() {
            typeof callback === 'function' && callback()
        }

        for (p in scriptInfo) {
            if (scriptInfo.hasOwnProperty(p) && p != "src") {
                tag[p] = scriptInfo[p];
            }
        }
        tag.src = scriptInfo.src;
        node.parentNode.insertBefore(tag, node);
    };

    gptApp.renderAds = function() {

        /* Render ad slot display after all targeting setup Adslots are pushed into gptApp array  */
        var len = gptApp.ads.length;
        for (var i = 0 ; i < len ; i++) {
            gptApp.ads[i]();
        }

        gptApp.ads.push = function(fn) {
            if ("function" === typeof fn) {
                setTimeout(fn, 0);
            }
        };
    };

    targetingComplete = function(event) {
        // enable and render ads for GPT services call that now all targeting finished
        completedTargeting[event] = true;

        if(!gptCallIntiated ) {
            // Measuring Each target event time to calculate timeout
            targetingEvtResponseTime.push(event+"|"+(Date.now() - adStart));

            if ((completedTargeting.lot && completedTargeting.amz && completedTargeting.ox) || event == "fallback") {
                gptCallIntiated = true;
                googletag.cmd.push(function(){
                    googletag.enableServices();
                    googletag.pubads().setTargeting("perf", targetingEvtResponseTime);
                });
                gptApp.renderAds();
            }
        }
    };

    oxDone = function(events) {
        /* Global Callback Function invoked by OpenX to inform price support */
        googletag.cmd.push(function() {
            //OPEN X GPT targeting Params
            try {
                OX.dfp_bidder.setOxTargeting(googletag.pubads().getSlots());
            } catch (e) {}
        });
        targetingComplete("ox");
    };

    // Google Publisher Tag
    attachScriptAsync({ src: "//www.googletagservices.com/tag/js/gpt.js"  }, function () {
        if (typeof window.DARCI !== 'undefined' && typeof window.DARCI.gptLoadedCallback === 'function')
            window.DARCI.gptLoadedCallback();
    });

    // AMAZON Bidder Tag
    attachScriptAsync({
        src: "//c.amazon-adsystem.com/aax2/amzn_ads.js",
        onload : function () {
            try {
                amznads.getAdsCallback('3067', function(){
                    googletag.cmd.push(function() {
                        // Amazon GPT targeting Params
                        amznads.setTargetingForGPTAsync('yip4');
                    });
                    targetingComplete("amz");
                }, 1000);
            } catch (e) { /* ignore */ }
        }
    });

    // LOTAME Measurement Tag For Dictionary.com
    attachScriptAsync({
        id: "LOTCC_4845",
        src: "//tags.crwdcntrl.net/c/4845/cc.js?ns=_cc4845",
        onload: function() {
            try {
                _cc4845.add("ctax", "Sites^Dictionary^Desktop Sites^dict^PageName^dict-wordoftheday^^dict-wordoftheday");
                _cc4845.bcp();
            } catch (e) {}
        }
    });

    // Lotame Audience Tag for IAC Network
    attachScriptAsync({
        src: "//ad.crwdcntrl.net/5/c=4736/pe=y/var=ccauds",
        onload :function() {
            googletag.cmd.push(function() {
                // Lotame GPT targeting Params
                var dartCC, lpid, ltpid, cci, audience;
                if (typeof(ccauds) != 'undefined' && ccauds.Profile) {
                    dartCC = [];
                    lpid = (typeof(ccauds.Profile.pid) != 'undefined') ? ccauds.Profile.pid : "";
                    ltpid = (typeof(ccauds.Profile.tpid) != 'undefined') ? ccauds.Profile.tpid : "";
                    audience = ((ccauds.Profile.Audiences || {})
                        .Audience) || [];
                    for (cci = 0; cci < audience.length; cci++) {
                        dartCC.push(audience[cci].abbr);
                    }
                    googletag.pubads().setTargeting("lpid", lpid).setTargeting("ltpid", ltpid).setTargeting("lseg", dartCC);
                }
            });
            targetingComplete("lot");
        }
    });

    // OPEN X Bidder Tag
    attachScriptAsync({
        src: "//ox-d.ask.servedbyopenx.com/w/1.0/jstag?nc=23219321-dictionary",
        onload: function() {
            setTimeout(function() {
                if (!('OX' in window)) {
                    targetingComplete("ox");
                }
            }, 500);
        }
    });

    // Fallback Method to invoke ad after 1000 miliseconds irrespective of targeting scripts

    setTimeout(function() {
        targetingComplete("fallback");
    }, 1500);

</script>

<script>
    function getRefValue(referrer) {
        if(referrer.indexOf('dictionary.com') == -1 && referrer.indexOf('thesaurus.com') == -1 && referrer.indexOf('reference.com') == -1) {
            return 'organic';
        }
        return 'direct';
    }
    function getRPV() {
        return Math.floor((Math.random()*100)+1).toString();
    }
    function getDCRefValue() {
        return encodeURIComponent(window.location.href);
    }
    function getDayofWeek() {
        return new Date().getDay().toString();
    }
    
    googletag.slots = {};
    googletag.cmd.push(function() {

        googletag.slots["leaderboard-1"] = googletag.defineSlot("/23219321/iac.dict.dictry.dw/wotd/homepage/channel", [[728,90],[970,90]], "leaderboard-1").addService(googletag.pubads()).setTargeting("pos", "top").setTargeting("pos2", "728x90_1").setTargeting("adCount", "leaderboard_1").setTargeting("tx1", "wotd").setTargeting("tx2", "homepage").setTargeting("ptype", "channel").setTargeting("wotdpage", "1");googletag.slots["medrec-1"] = googletag.defineSlot("/23219321/iac.dict.dictry.dw/wotd/homepage/channel", [300,250], "medrec-1").addService(googletag.pubads()).setTargeting("pos", "top").setTargeting("pos2", "300x250_1").setTargeting("adCount", "medrec_1").setTargeting("tx1", "wotd").setTargeting("tx2", "homepage").setTargeting("ptype", "channel").setTargeting("wotdpage", "1");
        googletag.pubads().setTargeting("ref", getRefValue(document.referrer)).setTargeting("lang","en").setTargeting("rpv", getRPV()).setTargeting("dc_ref", getDCRefValue()).setTargeting("loc", "US").setTargeting("ld", "0").setTargeting("dow", getDayofWeek());

        var get_request_identifier = (function () {
                return Math.random().toString(36).substr(2)
            })(),
            get_modified_page_url = (function () {
                sep = window.location.search.length >= 0 && window.location.href.indexOf("?") !== -1 ? "&" : "?";
                return window.location.href + sep + "rquid=" + get_request_identifier
            })();

        googletag.pubads().setTargeting("rquid",get_request_identifier);
        googletag.pubads().setTargeting("page_url",get_modified_page_url);

        
                var adtest = location.search.match(new RegExp("[\?\&]adtest=([^\&]*)(\&?)","i"));
        if(adtest != null && adtest[1] != "" ){
            googletag.pubads().setTargeting("adtest",adtest[1]);
        }

                        googletag.pubads().enableAsyncRendering();
                
        googletag.pubads().addEventListener('slotRenderEnded', handleSlotRender);
    });
</script>
